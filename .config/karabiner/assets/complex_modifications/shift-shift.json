{
    "title": "Shift-shift",
    "rules": [
      {
        "description": "Shift-shift",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "left_shift",
              "modifiers": {
                "optional": [
                  "any"
                ]
              }
            },
            "to": [
              {
                "key_code": "left_shift"
              }
            ],
            "to_if_alone": [
              {
                "key_code": "backslash",
                "modifiers": [
                    "left_control",
                    "left_command",
                    "left_shift"
                ]
              }
            ]
          },
          {
            "type": "basic",
            "from": {
              "key_code": "right_shift",
              "modifiers": {
                "optional": [
                  "any"
                ]
              }
            },
            "to": [
              {
                "key_code": "left_shift"
              }
            ],
            "to_if_alone": [
              {
                "key_code": "backslash",
                "modifiers": [
                    "left_control",
                    "left_option",
                    "left_command",
                    "left_shift"
                ]
              }
            ]
          }
        ]
      }
    ]
  }
  